{
  "name": "my-ticket-api",
  "version": "0.7.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "style": "prettier --check 'src/**/*.ts' 'test/**/*ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*ts'",
    "dev": "nodemon ./index.ts",
    "build": "rm -rf ./dist && tsc",
    "test": "npx vitest",
    "test:func": "NODE_ENV=test npx jest --no-cahce --runInBand --config ./test/jest.config.ts",
    "test:cov": "NODE_ENV=test npx jest --no-cahce --runInBand --config ./jest.config.ts --coverage",
    "start": "npx pm2-runtime start ecosystem.config.yml",
    "stop": "npx pm2 kill",
    "start:database": "docker-compose down && docker-compose up",
    "stop:database": "docker-compose down",
    "flow": "git branch -l",
    "flow:all": "git branch -l -a",
    "flow:rm": "git branch -D",
    "feature:start": "git flow feature start",
    "feature:finish": "git flow feature finish",
    "release:start": "VERSION=$(node -p \"require('./package').version\") && git flow release start $VERSION",
    "release:start:m": "git flow release start",
    "release:finish": "BRANCH=$(node -p \"require('./package').version\") && git flow release finish $BRANCH",
    "hotfix:start": "git flow hotfix start",
    "hotfix:finish": "git flow hotfix finish",
    "support:start": "git flow support start",
    "support:finish": "git flow support finish",
    "add:patch": "npm version patch && git status && git add --all  &&  git commit -m  \" Add new Patch in Version\" && git push",
    "add:minor": "npm version minor && git status && git add --all  &&  git commit -m  \" Add new Minor in Version\" && git push",
    "add:major": "npm version major && git status && git add --all  &&  git commit -m  \" Add new Major in Version\" && git push",
    "up:tags": "git push --follow-tags",
    "knex:migrate:make": "npx knex migrate:make --knexfile knexfile.ts -x ts",
    "knex:migrate:latest": "npx knex migrate:latest --knexfile knexfile.ts",
    "knex:migrate:rollback": "npx knex migrate:rollback --knexfile knexfile.ts"
  },
  "keywords": [
    "API",
    "TypeScript",
    "NodeJS",
    "Postgres",
    "Redis"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafalogan/my-ticket/my-ticket-api"
  },
  "homepage": "https://github.com/rafalogan/my-ticket/my-ticket-api/#README.md",
  "dependencies": {
    "aws-sdk": "^2.1218.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "express": "^4.18.1",
    "http": "^0.0.1-security",
    "http-status": "^1.5.3",
    "https": "^1.0.0",
    "is-empty": "^1.2.0",
    "jwt-simple": "^0.5.6",
    "knex": "^2.3.0",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "mongodb": "^4.9.1",
    "mongoose": "^6.6.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.7.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "pm2": "^5.2.0",
    "redis": "^4.3.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/faker": "^6.6.9",
    "@types/is-empty": "^1.2.1",
    "@types/jwt-simple": "^0.5.33",
    "@types/md5": "^2.3.2",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.7.18",
    "@types/nodemailer": "^6.4.6",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/pg": "^8.6.5",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^5.5.3",
    "faker-br": "^0.4.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "source-map": "^0.7.4",
    "sqlite3": "^5.1.1",
    "sucrase": "^3.27.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3",
    "vitest": "^0.23.2"
  },
  "_moduleAliases": {
    "@root": "./",
    "@deep": "src/some/very/deep/directory/or/file",
    "@my_module": "lib/some-file.js",
    "something": "src/foo"
  }
}
